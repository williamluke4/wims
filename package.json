{
  "name": "wims",
  "version": "0.1.3",
  "license": "MIT",
  "author": "William Luke <williamluke4@gmail.com",
  "private": false,
  "repository": "git@github:williamluke4/wims.git",
  "files": [
    "build"
  ],
  "bin": {
    "wims": "build/index.js"
  },
  "scripts": {
    "tsc": "tsc -d",
    "ncc": "ncc build dist/bin.js -o build",
    "build": "yarn tsc && yarn ncc",
    "version": "npm version `auto version` -m 'Bump version to: %s [skip ci]'",
    "publish": "npm publish && git push --follow-tags --set-upstream origin $branch",
    "release": "auto changelog && npm run version && npm run publish && auto release"
  },
  "peerDependencies": {},
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "auto": "^7.13.1",
    "tslib": "^1.10.0",
    "typescript": "^3.7.2"
  },
  "dependencies": {
    "@zeit/ncc": "^0.20.5",
    "arg": "^4.1.1",
    "chalk": "^2.4.2",
    "execa": "^3.2.0",
    "strip-indent": "^3.0.0"
  }
}
